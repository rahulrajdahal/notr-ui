{
  "name": "notr-ui",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*",
    "tools/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "publish": "changeset publish",
    "storybook": "storybook dev -p 6006 -c apps/.storybook",
    "build-storybook": "storybook build",
    "docs:dev": "vitepress dev apps/docs",
    "docs:build": "vitepress build apps/docs",
    "docs:preview": "vitepress preview apps/docs"
  },
  "devDependencies": {
    "@changesets/cli": "latest",
    "@chromatic-com/storybook": "^4.1.0",
    "@rahulrajdahal/notr-button": "workspace:*",
    "@rahulrajdahal/notr-core": "workspace:*",
    "@rahulrajdahal/notr-input": "workspace:*",
    "@rahulrajdahal/notr-passwordInput": "workspace:*",
    "@rahulrajdahal/notr-debounceInput": "workspace:*",
    "@storybook/addon-a11y": "9.1.0",
    "@storybook/addon-docs": "^9.1.0",
    "@storybook/react-vite": "^9.1.0",
    "prop-types": "^15.8.1",
    "storybook": "^9.1.0",
    "turbo": "latest",
    "vitepress": "^1.6.3"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@vitejs/plugin-react": "^4.7.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.6.5"
}
